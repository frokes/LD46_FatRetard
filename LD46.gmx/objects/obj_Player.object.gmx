            <kind>1</kind>
            <string>/// Get input and activate attacks
show_debug_message(powerup);
var offset_x, offset_y;
var parts = 25;

image_index = facing-1;

if(keyboard_check(ord("R")))
{
    surface_reset_target();
    game_restart();
}
if(keyboard_check(vk_space) &amp;&amp; rdyfire)
{
    switch(facing)
    {
        case 1:
            offset_x = spawn_x;
            offset_y = off_y;
            
        break;
        case 2:
            offset_x = 0;
            offset_y = -spawn_y;
        break;
        case 3:
            offset_x = -spawn_x;
            offset_y = off_y
        break;
        case 4:
            offset_x = 0;
            offset_y = spawn_y;
        break;
    }
    switch(powerup)
    {
        case 0: // regular attack (paper or rock throw)
            Projectile = instance_create(x + offset_x, y + offset_y, obj_Projectile);
            with(Projectile)
            {
                phy_rotation = other.facing * -90 +90 ;
                physics_apply_local_impulse(0,0,ProjSpd, 0);
            }
        break;
        
        case 1: // Fire medium AOE attack
            powerup = 0;
            for(i = -parts; i &lt; parts; i++)
            {
                flame = instance_create(x + offset_x, y + offset_y, obj_Flame);
                with(flame)
                {
                    phy_rotation = other.facing * -90 +90 + random_range(-50,50);
                    physics_apply_local_impulse(0,0,ProjSpd, random_range(-1,1) * ProjSpd*0.1);
                    physics_apply_angular_impulse(random_range(-2,2));
                }
            }
        break;
        
        case 2: // Lightning bolt single target long range
            powerup = 0;
            Projectile = instance_create(x + offset_x, y + offset_y, obj_lightning);
            with(Projectile)
            {
                phy_rotation = other.facing * -90 -180 ;
            }
        break;
        
        case 3: // Ice wide AOE slow
            powerup = 0;
            for(i = -parts; i &lt; parts; i++)
            {
                ice = instance_create(x + offset_x, y + offset_y, obj_Freeze);
                with(ice)
                {
                    phy_rotation = other.facing * -90 +90 + random_range(-50,50);
                    physics_apply_local_impulse(0,0,ProjSpd, 0);
                }
            }
            // Define particle systems
